{
  "mcp_servers": {
    "hubspot": {
      "command": "npx",
      "args": ["@hubspot/mcp-server"],
      "env": {
        "HUBSPOT_API_KEY": "${HUBSPOT_API_KEY}",
        "HUBSPOT_PORTAL_ID": "${HUBSPOT_PORTAL_ID}"
      },
      "capabilities": {
        "contacts": true,
        "tasks": true,
        "emails": true,
        "workflows": true
      }
    }
  },
  "ai_agents": {
    "assignee": "AI-Agents",
    "email": "ai-agents@salesswarm.ai"
  },
  "task_prefixes": {
    "qualify": "Qualify Lead",
    "outreach": "Send Outreach",
    "enrich": "Enrich Data",
    "followup": "Follow up"
  }
}